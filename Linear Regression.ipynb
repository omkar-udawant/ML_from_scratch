   "source": [
    "total_square_sum = 0 \n",
    "total_res_square_sum = 0\n",
    "for i in range(n):\n",
    "    pred = c + X[i] * m \n",
    "    total_square_sum += (Y[i] - y_mean) ** 2\n",
    "    total_res_square_sum += (Y[i] - pred) ** 2\n",
    "    \n",
    "r_square = 1 - (total_res_square_sum / total_square_sum)\n",
    "\n",
    "print(f'R2: {r_square}')"
   ]
